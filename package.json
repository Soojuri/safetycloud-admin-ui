    "copy-webpack-plugin": "^4.6.0",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.9.0",
    "element-ui": "^2.15.5",
    "flv.js": "^1.6.2",
    "hls.js": "^1.1.2",
